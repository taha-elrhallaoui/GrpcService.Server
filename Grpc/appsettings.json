{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Parrot": {
    "SupportedCultures": [
      "fr-FR",
      "nl-BE"
    ],
    "DefaultCulture": "fr-FR"
  }
}
